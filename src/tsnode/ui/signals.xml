<?xml version="1.0" encoding="UTF-8" ?>

<signals>

    <signal category="value.speed">
        <name>Geschwindigkeit</name>
        <description>Geschwindigkeit des Autos</description>
        <minValue>0</minValue>
        <maxValue>220</maxValue>
        <ticks>
            <tick>0</tick>
            <tick>20</tick>
            <tick>40</tick>
            <tick>60</tick>
            <tick>80</tick>
            <tick>100</tick>
            <tick>120</tick>
            <tick>140</tick>
            <tick>160</tick>
            <tick>180</tick>
            <tick>200</tick>
            <tick>220</tick>
        </ticks>

        <highlights>
            <highlight>
                <start>0</start>
                <end>50</end>
                <color>(0,0,0,0)</color>
            </highlight>
            <highlight>
                <start>50</start>
                <end>130</end>
                <color>(0,255,0,0.08)</color>
            </highlight>
            <highlight>
                <start>130</start>
                <end>220</end>
                <color>(255,0,0,0.08)</color>
            </highlight>
        </highlights>

        <unit>km/h</unit>
    </signal>
    <signal category="value.mass air flow">
        <name>MAF</name>
        <description>Luftmassenstrom</description>
        <minValue></minValue>
        <maxValue></maxValue>
        <unit>gramm</unit>
    </signal>
    <signal category="value.coolant temperature">
        <name>Kühlertemperatur</name>
        <description>Temperatur des Kühlers</description>
        <minValue>0</minValue>
        <maxValue>100</maxValue>
        <unit>°C</unit>
    </signal>
    <signal category="value.fuel pressure">
        <name>Treibstoffdruck</name>
        <description>Treibstoffdruck in bar</description>
        <minValue>0</minValue>
        <maxValue>10</maxValue>
        <unit>bar</unit>
    </signal>
    <signal category="value.RPM">
        <name>Drehzahl</name>
        <description>Motordrehzahl in RPM</description>
        <minValue>0</minValue>
        <maxValue>8000</maxValue>
        <ticks>
            <tick>0</tick>
            <tick>1000</tick>
            <tick>2000</tick>
            <tick>3000</tick>
            <tick>4000</tick>
            <tick>5000</tick>
            <tick>6000</tick>
            <tick>7000</tick>
            <tick>8000</tick>
        </ticks>
        <highlights>
            <highlight>
                <start>0</start>
                <end>5000</end>
                <color>(0,255,0,0.05)</color>
            </highlight>
            <highlight>
                <start>5000</start>
                <end>8000</end>
                <color>(255,0,0,0.3)</color>
            </highlight>
        </highlights>
        <unit>RPM</unit>
    </signal>
    <signal category="value.steering">
        <name>Lenkung</name>
        <description>Lenkungswinkel in Grad</description>
        <minValue>-30</minValue>
        <maxValue>30</maxValue>
        <unit>degree</unit>
    </signal>
    <signal category="value.intake air temperature">
        <name>Lufttemperatur</name>
        <description>Temperatur der eingezogenen Luft</description>
        <minValue>0</minValue>
        <maxValue>60</maxValue>
        <unit>°C</unit>
    </signal>
    <signal category="value.engine runtime">
        <name>Motorlaufzeit</name>
        <description>Zeit seit dem letzten Start des Motors</description>
        <minValue>0</minValue>
        <maxValue>3600</maxValue>
        <unit>Minuten</unit>
    </signal>
    <signal category="value.fuel">
        <name>Tankfüllstand</name>
        <description>Aktueller Tankfüllstand</description>
        <minValue>0</minValue>
        <maxValue>100</maxValue>
        <unit>%</unit>
        <picture></picture>
    </signal>
